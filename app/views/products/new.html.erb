<h1>Products#new</h1>
<p>Find me in app/views/products/new.html.erb</p>
<p>product新規作成</p>
<%= form_with model: @product do |f| %>
<table>
  <tr>
    <th><%= f.label :name, "名前" %></th>
    <td><%= f.text_field :name %></td>
  </tr>

  <tr>
    <th><%= f.label :image, "画像" %></th>
    <td><%= f.file_field :image %></td>
  </tr>
</table>
<div>
  <ul>
    <li><%= f.submit "登録完了する"%></li>
    <li><%= link_to "product一覧に戻る", :products %></li>
  </ul>
</div>
<% end %>